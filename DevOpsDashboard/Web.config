<configuration>
  <appSettings>
    <add key="ChartImageHandler" value="storage=file;timeout=20;dir=c:\TempImageFiles\;" />
    <add key="JobURL" value="http://192.168.109.4:8080/job/"/>
    <add key="AllJobURL" value="http://192.168.109.4:8080/api/xml"/>
    <add key="LastBuildApi" value="/lastBuild/api/xml"/>
    <add key="hostURL" value=""/>
